{
    "name": "tkotosz/fooapp",
    "type": "project",
    "repositories": [
        {
            "type": "path",
            "url": "fooapp-extension-api/",
            "options": {
                "symlink": false
            }
        }
    ],
    "require": {
        "composer/composer": "2.0.x-dev",
        "ext-json": "*",
        "symfony/console": "^5.0",
        "league/flysystem": "^1.0",
        "symfony/yaml": "^5.0",
        "tkotosz/fooapp-extension-api": "*",
        "symfony/dependency-injection": "^5.0",
        "symfony/config": "^5.0"
    },
    "authors": [
        {
            "name": "tkotosz",
            "email": "kotosy@gmail.com"
        }
    ],
    "bin": ["bin/fooapp"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Tkotosz\\FooApp\\": "src"
        }
    }
}
