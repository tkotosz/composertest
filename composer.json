{
    "name": "tkotosz/cli-app-wrapper",
    "type": "project",
    "repositories": [
        {
            "type": "path",
            "url": "cli-app-wrapper-api/"
        }
    ],
    "require": {
        "composer/composer": "2.0.x-dev",
        "tkotosz/cli-app-wrapper-api": "*",
        "ext-json": "*",
        "symfony/console": "^5.0",
        "league/flysystem": "^1.0",
        "symfony/yaml": "^5.0"
    },
    "authors": [
        {
            "name": "Tibor Kotosz",
            "email": "kotosy@gmail.com"
        }
    ],
    "bin": ["bin/fooapp"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Tkotosz\\CliAppWrapper\\": "src"
        }
    }
}
